      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc1access' name='Secure?' group='`${displayPath}`/Memory/PC 1' default='`${!(cmseAttribute eq "NON_SECURE")}`' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_ACCESS_TOOLTIP' />
