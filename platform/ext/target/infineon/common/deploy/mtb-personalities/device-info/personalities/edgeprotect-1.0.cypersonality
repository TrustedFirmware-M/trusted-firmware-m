    <ParamBool templateId='domain_crypto' id='unprivilegedAccess' name='Allow peripheral access in unprivileged mode' group='`${displayPath}`/Peripheral Protection' default='`${getParamValue("edge_protect", "isEPC4")}`' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_UNPRIV_ACCESS_TOOLTIP' />

    <ParamBool templateId='domain_crypto' id='pc0' name='PC 0' group='`${displayPath}`/Peripheral Protection/Accessible by:' default='false' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_PC_TOOLTIP' />
