      <Repeat count='$MAX_CORES' repeatId='coreIndex'>
        <ParamString templateId='domain_custom_$idx{domainIdx}' id='vcoretype$idx{coreIndex}' default='`${$idx &lt; vcore_count ? getCoreType(splitAt(vcore_names, $idx)) : ""}`' />
        <ParamBool templateId='domain_custom_$idx{domainIdx}' id='coreIsAccessible$idx{coreIndex}' default='`${$idx &lt; vcore_count &amp;&amp; ((includeM33core &amp;&amp; vcoretype$idx eq "$CM33") || (includeM55core &amp;&amp; vcoretype$idx eq "$CM55"))}`' />
      </Repeat>
    </Repeat>

