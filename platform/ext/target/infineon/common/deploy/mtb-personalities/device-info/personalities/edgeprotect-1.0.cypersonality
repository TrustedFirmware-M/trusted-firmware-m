        <ParamString templateId='domain_custom_$idx{domainIdx}' id='pc$idx{pcIndex}core' default='`${(((read_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0) || ((write_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0)) ? getOrDefault(getCustomData("/ProtectionContext[@key=\"" . $idx{pcIndex} . "\"]/@bus_master_ids"), "-1") : "-1"}`' />
      </Repeat>
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='includeM33core' default='`${((pc0core == $M33_BUS_MASTER_ID) || (pc1core == $M33_BUS_MASTER_ID) || (pc2core == $M33_BUS_MASTER_ID) || (pc3core == $M33_BUS_MASTER_ID) || (pc4core == $M33_BUS_MASTER_ID) || (pc5core == $M33_BUS_MASTER_ID) || (pc6core == $M33_BUS_MASTER_ID) || (pc7core == $M33_BUS_MASTER_ID))}`' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='includeM55core' default='`${((pc0core == $M55_BUS_MASTER_ID) || (pc1core == $M55_BUS_MASTER_ID) || (pc2core == $M55_BUS_MASTER_ID) || (pc3core == $M55_BUS_MASTER_ID) || (pc4core == $M55_BUS_MASTER_ID) || (pc5core == $M55_BUS_MASTER_ID) || (pc6core == $M55_BUS_MASTER_ID) || (pc7core == $M55_BUS_MASTER_ID))}`' />
