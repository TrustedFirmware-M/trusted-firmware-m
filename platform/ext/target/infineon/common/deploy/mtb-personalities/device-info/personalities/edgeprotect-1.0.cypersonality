    <ParamBool id='domain_ps_PLACED' name='Enable Protection Domain TFM_SP_PS' group='Protection Domains' default='`${tfm_services_its_ENABLED}`' visible='`${tfm_services_ps_ENABLED}`' editable='false' desc='Is the protection domain for the Protected Storage service enabled?' />
