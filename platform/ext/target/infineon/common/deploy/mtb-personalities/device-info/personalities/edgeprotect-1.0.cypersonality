    <Variable id='TFM_ITS_location' value='`${findMemoryRegionByName("TFM_ITS")}`' />
    <Variable id='TFM_ITS_size' value='`${getExposedMember(TFM_ITS_location, "size")}`' />
    <Variable id='TFM_ITS_memory' value='`${getExposedMember(TFM_ITS_location, "memoryId")}`' />
    <Variable id='TFM_ITS_domain_id' value='`${getExposedMember(TFM_ITS_location, "domain")}`' />
    <Variable id='TFM_ITS_domain_name' value='`${getExposedMember(getBlockFromInstance(TFM_ITS_domain_id), "displayName")}`' />

    <Variable id='TFM_PS_location' value='`${findMemoryRegionByName("TFM_PS")}`' />
    <Variable id='TFM_PS_size' value='`${getExposedMember(TFM_PS_location, "size")}`' />
    <Variable id='TFM_PS_memory' value='`${getExposedMember(TFM_PS_location, "memoryId")}`' />
    <Variable id='TFM_PS_domain_id' value='`${getExposedMember(TFM_PS_location, "domain")}`' />
    <Variable id='TFM_PS_domain_name' value='`${getExposedMember(getBlockFromInstance(TFM_PS_domain_id), "displayName")}`' />

    <Variable id='TFM_NV_COUNTERS_location' value='`${findMemoryRegionByName("TFM_NV_COUNTERS")}`' />
    <Variable id='TFM_NV_COUNTERS_size' value='`${getExposedMember(TFM_NV_COUNTERS_location, "size")}`' />
    <Variable id='TFM_NV_COUNTERS_memory' value='`${getExposedMember(TFM_NV_COUNTERS_location, "memoryId")}`' />
    <Variable id='TFM_NV_COUNTERS_domain_id' value='`${getExposedMember(TFM_NV_COUNTERS_location, "domain")}`' />
    <Variable id='TFM_NV_COUNTERS_domain_name' value='`${getExposedMember(getBlockFromInstance(TFM_NV_COUNTERS_domain_id), "displayName")}`' />

    <Variable id='m33s_data_location' value='`${findMemoryRegionByName("m33s_data")}`' />
    <Variable id='m33s_data_size' value='`${getExposedMember(m33s_data_location, "size")}`' />
    <Variable id='m33s_data_memory' value='`${getExposedMember(m33s_data_location, "memoryId")}`' />
    <Variable id='m33s_data_domain_id' value='`${getExposedMember(m33s_data_location, "domain")}`' />
    <Variable id='m33s_data_domain_name' value='`${getExposedMember(getBlockFromInstance(m33s_data_domain_id), "displayName")}`' />

    <Variable id='m33s_nvm_location' value='`${findMemoryRegionByName("m33s_nvm")}`' />
    <Variable id='m33s_nvm_size' value='`${getExposedMember(m33s_nvm_location, "size")}`' />
    <Variable id='m33s_nvm_memory' value='`${getExposedMember(m33s_nvm_location, "memoryId")}`' />
    <Variable id='m33s_nvm_domain_id' value='`${getExposedMember(m33s_nvm_location, "domain")}`' />
    <Variable id='m33s_nvm_domain_name' value='`${getExposedMember(getBlockFromInstance(m33s_nvm_domain_id), "displayName")}`' />

    <Variable id='m33s_code_location' value='`${findMemoryRegionByName("m33s_code")}`' />
    <Variable id='m33s_code_size' value='`${getExposedMember(m33s_code_location, "size")}`' />
    <Variable id='m33s_code_memory' value='`${getExposedMember(m33s_code_location, "memoryId")}`' />
    <Variable id='m33s_code_domain_id' value='`${getExposedMember(m33s_code_location, "domain")}`' />
    <Variable id='m33s_code_domain_name' value='`${getExposedMember(getBlockFromInstance(m33s_code_domain_id), "displayName")}`' />

    <Variable id='m33_nvm_location' value='`${findMemoryRegionByName("m33_nvm")}`' />
    <Variable id='m55_nvm_location' value='`${findMemoryRegionByName("m55_nvm")}`' />

    <Variable id='is_protection_enabled' value='`${isBlockUsed("$PROTECTION_LOC")}`' />
    <Variable id='is_protection_custom' value='`${getOrDefault(getExposedMember("$PROTECTION_LOC", "protectionLevel"), "") eq "SECURE_CUSTOM"}`' />
    <Variable id='is_context_switching_enabled' value='`${getExposedMember("$PROTECTION_LOC", "pcSwitch")}`' />
    <Variable id='peri0Enabled' value='`${isBlockUsed("peri[0]")}`' />
    <Variable id='peri1Enabled' value='`${isBlockUsed("peri[1]")}`' />
  </Variables>
  <DRCs>
    <!-- SAU -->
    <DRC type='WARNING' text='Maximum 7 out of 8 SAU regions can be enabled at once. One region shall be reserved for the TF-M.' condition='`${
        (hasExposedMember("$M33_SAU_LOC","enable0") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable0") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable1") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable1") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable2") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable2") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable3") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable3") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable4") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable4") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable5") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable5") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable6") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable6") eq true)) &amp;&amp;
        (hasExposedMember("$M33_SAU_LOC","enable7") &amp;&amp; (getExposedMember("$M33_SAU_LOC","enable7") eq true))
      }`' location='$M33_SAU_LOC' paramId='enable0' />

    <DRC type='WARNING' text='Non-Secure Callable SAU regions must not exist when TF-M is used.' condition='`${
        hasExposedMember("$M33_SAU_LOC","access$idx") &amp;&amp; (getExposedMember("$M33_SAU_LOC","access$idx") eq 3)
      }`' location='$M33_SAU_LOC' paramId='access$idx' repeatCount='$SAU_REGION_COUNT' />

    <!-- Internal Trusted Storage -->
    <DRC type='ERROR' text='Internal Trusted Storage Service needs memory region "TFM_ITS" for operation.' condition='`${
          tfm_services_its_ENABLED &amp;&amp;
          (!tfm_services_its_MEMORY_PLACED &amp;&amp; TFM_ITS_location eq "")
        }`' paramId='tfm_services_its_ENABLED'>
      <FixIt action='SET_PARAM' target='tfm_services_its_MEMORY_PLACED' value='true' valid='`${TFM_ITS_location eq ""}`' description='Enable memory region "TFM_ITS"' />
    </DRC>
    <DRC type='INFO' text='Internal Trusted Storage Service is disabled, so memory region "TFM_ITS" can be removed.' condition='`${
          !tfm_services_its_ENABLED &amp;&amp; tfm_services_its_MEMORY_PLACED
        }`'>
      <FixIt action='SET_PARAM' target='tfm_services_its_MEMORY_PLACED' value='false' valid='`${tfm_services_its_MEMORY_PLACED}`' description='Remove memory region "TFM_ITS"' />
    </DRC>
    <DRC type='WARNING' text='Memory region "TFM_ITS" size "`${TFM_ITS_size}`" is too small.' condition='`${TFM_ITS_location ne "" &amp;&amp; TFM_ITS_size &lt; $MEM_REGION_SIZE_TFM_ITS}`'>
      <FixIt action='SET_PARAM' target='`${TFM_ITS_location}`.size' value='$MEM_REGION_SIZE_TFM_ITS' valid='true' description='Change "TFM_ITS" size to "$MEM_REGION_SIZE_TFM_ITS"' />
    </DRC>
    <DRC type='WARNING' text='Memory region "TFM_ITS" domain "`${TFM_ITS_domain_name}`" is not supported by TF-M.' condition='`${TFM_ITS_domain_id ne IFX_DOMAIN_ITS_domain_id}`'>
      <FixIt action='SET_PARAM' target='`${TFM_ITS_location}`.domain' value='`${IFX_DOMAIN_ITS_domain_id}`' valid='true' description='Change memory region "TFM_ITS" domain to "TFM_SP_ITS"' />
    </DRC>
    <DRC type='WARNING' text='Memory region "TFM_ITS" memory "`${TFM_ITS_memory}`" is not supported by TF-M.' condition='`${TFM_ITS_memory ne "RRAM"}`'>
      <FixIt action='SET_PARAM' target='`${TFM_ITS_location}`.memoryId' value='RRAM' valid='true' description='Change "TFM_ITS" memory to "RRAM"' />
    </DRC>

    <!-- Protected Storage  -->
    <DRC type='ERROR' text='Protected Storage Service needs memory region "TFM_PS" for operation.' condition='`${
          tfm_services_ps_ENABLED &amp;&amp;
          (!tfm_services_ps_MEMORY_PLACED &amp;&amp; TFM_PS_location eq "")
        }`' paramId='tfm_services_ps_ENABLED'>
      <FixIt action='SET_PARAM' target='tfm_services_ps_MEMORY_PLACED' value='true' valid='`${TFM_PS_location eq ""}`' description='Enable memory region "TFM_PS"' />
    </DRC>
    <DRC type='INFO' text='Protected Storage Service is disabled, so memory region "TFM_PS" can be removed.' condition='`${
          !tfm_services_ps_ENABLED &amp;&amp; tfm_services_ps_MEMORY_PLACED
        }`'>
      <FixIt action='SET_PARAM' target='tfm_services_ps_MEMORY_PLACED' value='false' valid='`${tfm_services_ps_MEMORY_PLACED}`' description='Remove memory region "TFM_PS"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_PS" size "`${TFM_PS_size}`" is too small.' condition='`${TFM_PS_location ne "" &amp;&amp; TFM_PS_size &lt; $MEM_REGION_SIZE_TFM_PS}`'>
      <FixIt action='SET_PARAM' target='`${TFM_PS_location}`.size' value='$MEM_REGION_SIZE_TFM_PS' valid='true' description='Change "TFM_PS" size to "$MEM_REGION_SIZE_TFM_PS"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_PS" domain "`${TFM_PS_domain_name}`" is not supported by TF-M.' condition='`${TFM_PS_domain_id ne IFX_DOMAIN_PS_domain_id}`'>
      <FixIt action='SET_PARAM' target='`${TFM_PS_location}`.domain' value='`${IFX_DOMAIN_PS_domain_id}`' valid='true' description='Change memory region "TFM_PS" domain to "TFM_SP_PS"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_PS" memory "`${TFM_PS_memory}`" is not supported by TF-M.' condition='`${TFM_PS_memory ne "RRAM"}`'>
      <FixIt action='SET_PARAM' target='`${TFM_PS_location}`.memoryId' value='RRAM' valid='true' description='Change "TFM_PS" memory to "RRAM"' />
    </DRC>

    <!-- TFM_NV_COUNTERS -->
    <DRC type='ERROR' text='Platform Service needs memory region "TFM_NV_COUNTERS" for operation.' condition='`${
          isEPC2 &amp;&amp; !tfm_services_platform_MEMORY_PLACED &amp;&amp; TFM_NV_COUNTERS_location eq ""
        }`' paramId='tfm_services_platform_MEMORY_PLACED'>
      <FixIt action='SET_PARAM' target='tfm_services_platform_MEMORY_PLACED' value='true' valid='`${TFM_NV_COUNTERS_location eq ""}`' description='Enable memory region "TFM_NV_COUNTERS"' />
    </DRC>
    <DRC type='INFO' text='Platform Service is disabled, so memory region "TFM_NV_COUNTERS" can be removed.' condition='`${
          !isEPC2 &amp;&amp; tfm_services_platform_MEMORY_PLACED
        }`'>
      <FixIt action='SET_PARAM' target='tfm_services_platform_MEMORY_PLACED' value='false' valid='`${tfm_services_platform_MEMORY_PLACED}`' description='Remove memory region "TFM_NV_COUNTERS"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_NV_COUNTERS" size "`${TFM_NV_COUNTERS_size}`" is too small.' condition='`${TFM_NV_COUNTERS_location ne "" &amp;&amp; TFM_NV_COUNTERS_size &lt; $MEM_REGION_SIZE_TFM_NV_COUNTERS}`'>
      <FixIt action='SET_PARAM' target='`${TFM_NV_COUNTERS_location}`.size' value='$MEM_REGION_SIZE_TFM_NV_COUNTERS' valid='true' description='Change "TFM_NV_COUNTERS" size to "$MEM_REGION_SIZE_TFM_NV_COUNTERS"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_NV_COUNTERS" domain "`${TFM_NV_COUNTERS_domain_name}`" is not supported by TF-M.' condition='`${TFM_NV_COUNTERS_domain_id ne M33S_domain_id}`'>
      <FixIt action='SET_PARAM' target='`${TFM_NV_COUNTERS_location}`.domain' value='`${M33S_domain_id}`' valid='true' description='Change memory region "TFM_NV_COUNTERS" domain to "M33S"' />
    </DRC>
    <DRC type='INFO' text='Memory region "TFM_NV_COUNTERS" memory "`${TFM_NV_COUNTERS_memory}`" is not supported by TF-M.' condition='`${TFM_NV_COUNTERS_memory ne "RRAM"}`'>
      <FixIt action='SET_PARAM' target='`${TFM_NV_COUNTERS_location}`.memoryId' value='RRAM' valid='true' description='Change "TFM_NV_COUNTERS" memory to "RRAM"' />
    </DRC>

    <!-- m33s_data -->
    <DRC type='WARNING' text='TF-M SPE Data Region requires memory region "m33s_data" but it is not present.' condition='`${!m33s_data_MEMORY_PLACED  &amp;&amp; m33s_data_location eq ""}`' paramId='m33s_data_MEMORY_PLACED'>
      <FixIt action='SET_PARAM' target='m33s_data_MEMORY_PLACED' value='true' valid='true' description='Enable memory region "m33s_data"' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_data" size "`${m33s_data_size}`" is too small.' condition='`${m33s_data_location ne "" &amp;&amp; m33s_data_size &lt; $MEM_REGION_SIZE_M33S_DATA}`'>
      <FixIt action='SET_PARAM' target='`${m33s_data_location}`.size' value='$MEM_REGION_SIZE_M33S_DATA' valid='true' description='Change "m33s_data" size to "$MEM_REGION_SIZE_M33S_DATA"' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_data" domain "`${m33s_data_domain_name}`" is not supported by TF-M.' condition='`${m33s_data_domain_id ne M33S_domain_id}`'>
      <FixIt action='SET_PARAM' target='`${m33s_data_location}`.domain' value='`${M33S_domain_id}`' valid='true' description='Change memory region "m33s_data" domain to "M33S"' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_data" memory "`${m33s_data_memory}`" is not supported by TF-M.' condition='`${m33s_data_memory ne "SRAM" &amp;&amp; m33s_data_memory ne "SOCMEM_RAM"}`'>
      <FixIt action='SET_PARAM' target='`${m33s_data_location}`.memoryId' value='SRAM' valid='true' description='Change "m33s_data" memory to "SRAM" (preferred)' />
      <FixIt action='SET_PARAM' target='`${m33s_data_location}`.memoryId' value='SOCMEM_RAM' valid='true' description='Change "m33s_data" memory to "SOCMEM_RAM"' />
    </DRC>

    <!-- m33s_nvm -->
    <DRC type='WARNING' text='TF-M SPE Code Region requires memory region "m33s_nvm" but it is not present.' condition='`${!m33s_nvm_MEMORY_PLACED &amp;&amp; m33s_nvm_location eq ""}`' paramId='m33s_nvm_MEMORY_PLACED'>
      <FixIt action='SET_PARAM' target='m33s_nvm_MEMORY_PLACED' value='true' valid='true' description='Enable memory region "m33s_nvm"' />
    </DRC>
    <DRC type='WARNING' text='TF-M NSPE requries one of memory regions [m33_nvm, m55_nvm] but none of them exist.' condition='`${m33_nvm_location eq "" &amp;&amp; m55_nvm_location eq ""}`' location='$MEMORY_LOC' />

    <DRC type='INFO' text='Memory region "m33s_nvm" size "`${m33s_nvm_size}`" is too small.' condition='`${m33s_nvm_location ne "" &amp;&amp; m33s_nvm_size &lt; $MEM_REGION_SIZE_M33S_NVM}`'>
      <FixIt action='SET_PARAM' target='`${m33s_nvm_location}`.size' value='$MEM_REGION_SIZE_M33S_NVM' valid='true' description='Change "m33s_nvm" size to "$MEM_REGION_SIZE_M33S_NVM"' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_nvm" domain "`${m33s_nvm_domain_name}`" is not supported by TF-M.' condition='`${m33s_nvm_domain_id ne IFX_DOMAIN_SPE_domain_id}`'>
      <FixIt action='SET_PARAM' target='`${m33s_nvm_location}`.domain' value='`${IFX_DOMAIN_SPE_domain_id}`' valid='true' description='Change memory region "m33s_nvm" domain to "M33S_CODE"' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_nvm" memory "`${m33s_nvm_memory}`" is not supported by TF-M.' condition='`${m33s_nvm_memory ne first_available_smif_memory_or_rram &amp;&amp; m33s_nvm_memory ne "RRAM"}`'>
      <FixIt action='SET_PARAM' target='`${m33s_nvm_location}`.memoryId' value='`${first_available_smif_memory_or_rram}`' valid='true' description='Change "m33s_nvm" memory to "`${first_available_smif_memory_or_rram}`"' />
      <FixIt action='SET_PARAM' target='`${m33s_nvm_location}`.memoryId' value='RRAM' valid='`${first_available_smif_memory_or_rram ne "RRAM"}`' description='Change "m33s_nvm" memory to "RRAM"' />
    </DRC>
    <DRC type='INFO' text='Allow to execute TF-M image from m33s_code should be enabled for EPC4 devices to satisfy PSA Level 3 requirements.' condition='`${isEPC4 &amp;&amp; !use_separate_execute_region}`' paramId='use_separate_execute_region'>
      <FixIt action='SET_PARAM' target='use_separate_execute_region' value='true' valid='`${!use_separate_execute_region}`' description='Allow to execute TF-M image from m33s_code' />
    </DRC>

    <!-- m33s_code -->
    <DRC type='ERROR' text='When TF-M image execution from memory region "m33s_code" is turned on (default for EPC4 devices), the memory region "m33s_code" must exist. Enable the region via Fix or create it manually in Memory tab of the Device Configurator.' condition='`${use_separate_execute_region &amp;&amp; !m33s_code_MEMORY_PLACED &amp;&amp; m33s_code_location eq ""}`' paramId='use_separate_execute_region'>
      <FixIt action='SET_PARAM' target='m33s_code_MEMORY_PLACED' value='true' valid='true' description='Enable memory region "m33s_code"' />
      <FixIt action='SET_PARAM' target='use_separate_execute_region' value='false' valid='`${use_separate_execute_region}`' description='Disallow to execute TF-M image from m33s_code' />
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_code" size "`${m33s_code_size}`" is too small.' condition='`${use_separate_execute_region &amp;&amp; m33s_code_location ne "" &amp;&amp; m33s_code_size &lt; $MEM_REGION_SIZE_M33S_CODE}`'>
      <FixIt action='SET_PARAM' target='`${m33s_code_location}`.size' value='$MEM_REGION_SIZE_M33S_CODE' valid='true' description='Change "m33s_code" size to "$MEM_REGION_SIZE_M33S_CODE"' />
    </DRC>
    <DRC type='ERROR' text='Memory region "m33s_code" domain "`${m33s_code_domain_name}`" is not supported by TF-M.' condition='`${use_separate_execute_region &amp;&amp; m33s_code_location ne "" &amp;&amp; (!domain_spe_PLACED || m33s_code_domain_id ne IFX_DOMAIN_SPE_domain_id)}`'>
      <FixItBatch description='Change memory region "m33s_code" domain to "M33S_CODE"'>
        <FixIt action='SET_PARAM' target='domain_spe_PLACED' value='true' valid='`${!domain_spe_PLACED}`' />
        <FixIt action='SET_PARAM' target='`${m33s_code_location}`.domain' value='`${IFX_DOMAIN_SPE_domain_id}`' valid='true' />
      </FixItBatch>
    </DRC>
    <DRC type='INFO' text='Memory region "m33s_code" memory "`${m33s_code_memory}`" is not supported by TF-M.' condition='`${use_separate_execute_region &amp;&amp; m33s_code_memory ne "SRAM" &amp;&amp; m33s_code_memory ne "SOCMEM_RAM"}`'>
      <FixIt action='SET_PARAM' target='`${m33s_code_location}`.memoryId' value='SRAM' valid='true' description='Change "m33s_code" memory to "SRAM" (preferred)' />
      <FixIt action='SET_PARAM' target='`${m33s_code_location}`.memoryId' value='SOCMEM_RAM' valid='true' description='Change "m33s_code" memory to "SOCMEM_RAM"' />
    </DRC>

    <!-- Custom Services -->
    <DRC type='INFO' text='Custom service "`${tfm_services_custom_$idx_NAME}`" needs Protection Domain "`${tfm_services_custom_$idx_DOMAIN_NAME}`" for operation.' condition='`${
      $idx &lt; tfm_general_CUSTOM_SERVICE_COUNT &amp;&amp;
      tfm_services_custom_$idx_ENABLED &amp;&amp;
      !tfm_services_custom_$idx_REMOVED &amp;&amp;
      !domain_custom_$idx_ENABLED
    }`' repeatCount='$MAX_CUSTOM_SERVICES'>
      <FixIt action='SET_PARAM' target='domain_custom_$idx_ENABLED' value='true' valid='true' description='Enable Protection Domain "`${tfm_services_custom_$idx_DOMAIN_NAME}`"' />
    </DRC>
    <DRC type='INFO' text='Custom service "`${tfm_services_custom_$idx_NAME}`" is disabled so hardware resources can be removed.' condition='`${
      $idx &lt; tfm_general_CUSTOM_SERVICE_COUNT &amp;&amp;
      !tfm_services_custom_$idx_ENABLED &amp;&amp;
      domain_custom_$idx_ENABLED
    }`' repeatCount='$MAX_CUSTOM_SERVICES'>
      <FixIt action='SET_PARAM' target='domain_custom_$idx_ENABLED' value='false' valid='true' description='Disable Protection Domain "`${tfm_services_custom_$idx_DOMAIN_NAME}`"' />
    </DRC>
    <DRC type='INFO' text='Custom service "`${tfm_services_custom_$idx_NAME}`" is removed so Protection Domain "`${tfm_services_custom_$idx_DOMAIN_NAME}`" must be removed.' condition='`${
      $idx &lt; tfm_general_CUSTOM_SERVICE_COUNT &amp;&amp;
      tfm_services_custom_$idx_REMOVED &amp;&amp;
      domain_custom_$idx_ENABLED
    }`' repeatCount='$MAX_CUSTOM_SERVICES'>
      <FixItBatch description='Unassign memory from Protection Domain "`${tfm_services_custom_$idx_DOMAIN_NAME}`" and remove it.'>
        <FixIt action='SET_PARAM' target='domain_custom_$idx_ENABLED' value='false' valid='true' description='Remove Protection Domain' />
      </FixItBatch>
    </DRC>

    <!-- PPC -->
    <DRC type='ERROR' condition='`${!is_protection_enabled || !is_protection_custom || (isEPC4 &amp;&amp; !is_context_switching_enabled)}`' text='TF-M requires Protection to be enabled and configured'>
      <FixItBatch description='Enable and configure Protection'>
        <FixIt action='ENABLE_BLOCK' target='$PROTECTION_LOC' valid='`${!is_protection_enabled}`' />
        <FixIt action='SET_PARAM' target='$PROTECTION_LOC.type' value='SECURE_CUSTOM' valid='`${!is_protection_custom}`' />
        <FixIt action='SET_PARAM' target='$PROTECTION_LOC.pcSwitch' value='true' valid='`${isEPC4 &amp;&amp; !is_context_switching_enabled}`' />
      </FixItBatch>
    </DRC>
    <DRC type='ERROR' condition='`${!peri0Enabled || !peri1Enabled}`' text='TF-M requires configuring PPCs'>
      <FixItBatch description='Enable PPCs'>
        <FixIt action='ENABLE_BLOCK' target='peri[0]' valid='`${!peri0Enabled}`' />
        <FixIt action='ENABLE_BLOCK' target='peri[1]' valid='`${!peri1Enabled}`' />
      </FixItBatch>
    </DRC>
  </DRCs>
  <ConfigFirmware>
    <ConfigFile name='ifx_tfm_image_config.h' include='true' visibleInPreview='true'>
      <Line value='/* This file is automatically generated. */' />
      <Line value='#if !defined(IFX_TFM_IMAGE_CONFIG_H)' />
      <Line value='#define IFX_TFM_IMAGE_CONFIG_H' />
      <Line value='' />
      <Line value='// Allows execution of TF-M image from different memory areas' />
      <Line value='#define IFX_TFM_IMAGE_EXECUTE_REGION_NAME     `${execution_region}`' />
      <Line value='' />
      <Line value='#define IFX_TFM_IMAGE_EXECUTE_OFFSET          CYMEM_CM33_0_S_`${execution_region}`_OFFSET' />
      <Line value='#define IFX_TFM_IMAGE_EXECUTE_ADDR            CYMEM_CM33_0_S_`${execution_region}`_C_S_START' />
      <Line value='#define IFX_TFM_IMAGE_EXECUTE_SIZE            CYMEM_CM33_0_S_`${execution_region}`_SIZE' />
      <Line value='#define IFX_TFM_IMAGE_EXECUTE_LOCATION        CYMEM_CM33_0_S_`${execution_region}`_LOCATION' />
      <Line value='' />
      <Line value='#endif /* IFX_TFM_IMAGE_CONFIG_H */' />
    </ConfigFile>
  </ConfigFirmware>

