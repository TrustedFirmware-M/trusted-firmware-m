    <ParamString id='first_available_smif_memory_or_rram' name='first_available_smif_memory_or_rram' default='`${
      splitFind(memories, "SMIF0MEM0") != -1 ? "SMIF0MEM0" :
      splitFind(memories, "SMIF0MEM1") != -1 ? "SMIF0MEM1" :
      splitFind(memories, "SMIF0MEM2") != -1 ? "SMIF0MEM2" :
      splitFind(memories, "SMIF0MEM3") != -1 ? "SMIF0MEM3" :
      splitFind(memories, "SMIF1MEM0") != -1 ? "SMIF1MEM0" :
      splitFind(memories, "SMIF1MEM1") != -1 ? "SMIF1MEM1" :
      splitFind(memories, "SMIF1MEM2") != -1 ? "SMIF1MEM2" :
      splitFind(memories, "SMIF1MEM3") != -1 ? "SMIF1MEM3" :
      "RRAM"
    }`' />

