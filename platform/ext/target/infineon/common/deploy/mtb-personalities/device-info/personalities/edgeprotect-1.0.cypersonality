    <ParamBool templateId='domain_crypto' id='pc7read' name='Allow reads' group='`${displayPath}`/Memory/PC 7' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_READ_TOOLTIP' />
