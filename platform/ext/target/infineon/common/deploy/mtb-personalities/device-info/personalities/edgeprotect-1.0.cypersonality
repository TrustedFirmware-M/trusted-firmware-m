        ((pc4read ? 1 : 0) &lt;&lt; 4u) |
        ((pc5read ? 1 : 0) &lt;&lt; 5u) |
        ((pc6read ? 1 : 0) &lt;&lt; 6u) |
        ((pc7read ? 1 : 0) &lt;&lt; 7u) }`' />
    <ParamString templateId='domain_spe' id='write_perm' default='`${
        (pc0write ? 1 : 0) |
        ((pc1write ? 1 : 0) &lt;&lt; 1u) |
        ((pc2write ? 1 : 0) &lt;&lt; 2u) |
        ((pc3write ? 1 : 0) &lt;&lt; 3u) |
        ((pc4write ? 1 : 0) &lt;&lt; 4u) |
        ((pc5write ? 1 : 0) &lt;&lt; 5u) |
        ((pc6write ? 1 : 0) &lt;&lt; 6u) |
        ((pc7write ? 1 : 0) &lt;&lt; 7u) }`' />
    <ParamString templateId='domain_spe' id='ns_perm' default='`${
        (pc0ns ? 1 : 0) |
        ((pc1ns ? 1 : 0) &lt;&lt; 1u) |
        ((pc2ns ? 1 : 0) &lt;&lt; 2u) |
        ((pc3ns ? 1 : 0) &lt;&lt; 3u) |
        ((pc4ns ? 1 : 0) &lt;&lt; 4u) |
        ((pc5ns ? 1 : 0) &lt;&lt; 5u) |
        ((pc6ns ? 1 : 0) &lt;&lt; 6u) |
        ((pc7ns ? 1 : 0) &lt;&lt; 7u) }`' />
    <Repeat count='$MAX_PC' repeatId='pcIndex'>
      <ParamString templateId='domain_spe' id='pc$idx{pcIndex}core' default='`${(((read_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0) || ((write_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0)) ? getOrDefault(getCustomData("/ProtectionContext[@key=\"" . $idx{pcIndex} . "\"]/@bus_master_ids"), "-1") : "-1"}`' />
    </Repeat>
    <ParamBool templateId='domain_spe' id='includeM33core' default='`${((pc0core == $M33_BUS_MASTER_ID) || (pc1core == $M33_BUS_MASTER_ID) || (pc2core == $M33_BUS_MASTER_ID) || (pc3core == $M33_BUS_MASTER_ID) || (pc4core == $M33_BUS_MASTER_ID) || (pc5core == $M33_BUS_MASTER_ID) || (pc6core == $M33_BUS_MASTER_ID) || (pc7core == $M33_BUS_MASTER_ID))}`' />
    <ParamBool templateId='domain_spe' id='includeM55core' default='`${((pc0core == $M55_BUS_MASTER_ID) || (pc1core == $M55_BUS_MASTER_ID) || (pc2core == $M55_BUS_MASTER_ID) || (pc3core == $M55_BUS_MASTER_ID) || (pc4core == $M55_BUS_MASTER_ID) || (pc5core == $M55_BUS_MASTER_ID) || (pc6core == $M55_BUS_MASTER_ID) || (pc7core == $M55_BUS_MASTER_ID))}`' />
    <ParamString templateId='domain_spe' id='vcore_count' default='`${getVirtualCoreCount()}`' />
    <ParamString templateId='domain_spe' id='vcore_names' default='`${getVirtualCoreDefineNames()}`' />
    <Repeat count='$MAX_CORES'>
      <ParamString templateId='domain_spe' id='vcoretype$idx' default='`${$idx &lt; vcore_count ? getCoreType(splitAt(vcore_names, $idx)) : ""}`' />
      <ParamBool templateId='domain_spe' id='coreIsAccessible$idx' default='`${$idx &lt; vcore_count &amp;&amp; ((includeM33core &amp;&amp; vcoretype$idx eq "$CM33") || (includeM55core &amp;&amp; vcoretype$idx eq "$CM55"))}`' />
    </Repeat>

    <!-- IFX_DOMAIN_CUSTOM -->
    <Repeat count='$MAX_CUSTOM_SERVICES' repeatId='domainIdx'>
      <ParamBool id='domain_custom_$idx_ENABLED' name='Enable Protection Domain `${tfm_services_custom_$idx_DOMAIN_NAME}`' group='Protection Domains' default='`${$idx &lt; tfm_general_CUSTOM_SERVICE_COUNT &amp;&amp; tfm_services_custom_$idx_ENABLED &amp;&amp; !tfm_services_custom_$idx_REMOVED}`' visible='`${$idx &lt; tfm_general_CUSTOM_SERVICE_COUNT &amp;&amp; !tfm_services_custom_$idx_REMOVED}`' editable='`${show_detailed_configuration}`' desc='$PROTECTION_DOMAIN_ENABLED_TOOLTIP' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='displayPath' default='Protection Domains/Enable Protection Domain `${getParamValue("edge_protect", "tfm_services_custom_$idx_DOMAIN_NAME")}`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='displayName' default='`${getParamValue("edge_protect", "tfm_services_custom_$idx_DOMAIN_NAME")}`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='show_detailed_configuration' default='`${getParamValue("edge_protect", "tfm_services_custom_$idx_ENABLED") &amp;&amp; getParamValue("edge_protect", "show_detailed_configuration")}`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='location' default='`${INST_LOC}`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='domainId' default='`${INST_ID}`' />
      <ParamChoice templateId='domain_custom_$idx{domainIdx}' id='cmseAttribute' name='Access' group='`${displayPath}`' default='SECURE' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_CMSE_TOOLTIP'>
        <Entry name='Secure' value='SECURE' visible='true' />
        <Entry name='Non-Secure Callable' value='NON_SECURE_CALLABLE' visible='true' />
        <Entry name='Non-Secure' value='NON_SECURE' visible='true' />
      </ParamChoice>
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='unprivilegedAccess' name='Allow peripheral access in unprivileged mode' group='`${displayPath}`/Peripheral Protection' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_UNPRIV_ACCESS_TOOLTIP' />

      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc0' name='PC 0' group='`${displayPath}`/Peripheral Protection/Accessible by:' default='false' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_PC_TOOLTIP' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc0read' name='Allow reads' group='`${displayPath}`/Memory/PC 0' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_READ_TOOLTIP' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc0write' name='Allow writes' group='`${displayPath}`/Memory/PC 0' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_WRITE_TOOLTIP' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc0access' name='Secure?' group='`${displayPath}`/Memory/PC 0' default='`${!(cmseAttribute eq "NON_SECURE")}`' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_ACCESS_TOOLTIP' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc0ns' default='`${!pc0access}`' />

      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc1' name='PC 1' group='`${displayPath}`/Peripheral Protection/Accessible by:' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_PC_TOOLTIP' />
      <ParamBool templateId='domain_custom_$idx{domainIdx}' id='pc1read' name='Allow reads' group='`${displayPath}`/Memory/PC 1' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_READ_TOOLTIP' />
