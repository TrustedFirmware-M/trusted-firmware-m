    <ParamBool templateId='domain_crypto' id='pc7ns' default='`${!pc7access}`' />
    <ParamString templateId='domain_crypto' id='read_perm' default='`${
        (pc0read ? 1 : 0) |
        ((pc1read ? 1 : 0) &lt;&lt; 1u) |
        ((pc2read ? 1 : 0) &lt;&lt; 2u) |
        ((pc3read ? 1 : 0) &lt;&lt; 3u) |
        ((pc4read ? 1 : 0) &lt;&lt; 4u) |
        ((pc5read ? 1 : 0) &lt;&lt; 5u) |
        ((pc6read ? 1 : 0) &lt;&lt; 6u) |
        ((pc7read ? 1 : 0) &lt;&lt; 7u) }`' />
    <ParamString templateId='domain_crypto' id='write_perm' default='`${
        (pc0write ? 1 : 0) |
        ((pc1write ? 1 : 0) &lt;&lt; 1u) |
        ((pc2write ? 1 : 0) &lt;&lt; 2u) |
        ((pc3write ? 1 : 0) &lt;&lt; 3u) |
        ((pc4write ? 1 : 0) &lt;&lt; 4u) |
        ((pc5write ? 1 : 0) &lt;&lt; 5u) |
        ((pc6write ? 1 : 0) &lt;&lt; 6u) |
        ((pc7write ? 1 : 0) &lt;&lt; 7u) }`' />
    <ParamString templateId='domain_crypto' id='ns_perm' default='`${
        (pc0ns ? 1 : 0) |
        ((pc1ns ? 1 : 0) &lt;&lt; 1u) |
        ((pc2ns ? 1 : 0) &lt;&lt; 2u) |
        ((pc3ns ? 1 : 0) &lt;&lt; 3u) |
        ((pc4ns ? 1 : 0) &lt;&lt; 4u) |
        ((pc5ns ? 1 : 0) &lt;&lt; 5u) |
        ((pc6ns ? 1 : 0) &lt;&lt; 6u) |
        ((pc7ns ? 1 : 0) &lt;&lt; 7u) }`' />
    <Repeat count='$MAX_PC' repeatId='pcIndex'>
      <ParamString templateId='domain_crypto' id='pc$idx{pcIndex}core' default='`${(((read_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0) || ((write_perm &amp; (0x1 &lt;&lt; $idx{pcIndex})) &gt; 0)) ? getOrDefault(getCustomData("/ProtectionContext[@key=\"" . $idx{pcIndex} . "\"]/@bus_master_ids"), "-1") : "-1"}`' />
    </Repeat>
    <ParamBool templateId='domain_crypto' id='includeM33core' default='`${((pc0core == $M33_BUS_MASTER_ID) || (pc1core == $M33_BUS_MASTER_ID) || (pc2core == $M33_BUS_MASTER_ID) || (pc3core == $M33_BUS_MASTER_ID) || (pc4core == $M33_BUS_MASTER_ID) || (pc5core == $M33_BUS_MASTER_ID) || (pc6core == $M33_BUS_MASTER_ID) || (pc7core == $M33_BUS_MASTER_ID))}`' />
    <ParamBool templateId='domain_crypto' id='includeM55core' default='`${((pc0core == $M55_BUS_MASTER_ID) || (pc1core == $M55_BUS_MASTER_ID) || (pc2core == $M55_BUS_MASTER_ID) || (pc3core == $M55_BUS_MASTER_ID) || (pc4core == $M55_BUS_MASTER_ID) || (pc5core == $M55_BUS_MASTER_ID) || (pc6core == $M55_BUS_MASTER_ID) || (pc7core == $M55_BUS_MASTER_ID))}`' />
    <ParamString templateId='domain_crypto' id='vcore_count' default='`${getVirtualCoreCount()}`' />
    <ParamString templateId='domain_crypto' id='vcore_names' default='`${getVirtualCoreDefineNames()}`' />
    <Repeat count='$MAX_CORES'>
      <ParamString templateId='domain_crypto' id='vcoretype$idx' default='`${$idx &lt; vcore_count ? getCoreType(splitAt(vcore_names, $idx)) : ""}`' />
      <ParamBool templateId='domain_crypto' id='coreIsAccessible$idx' default='`${$idx &lt; vcore_count &amp;&amp; ((includeM33core &amp;&amp; vcoretype$idx eq "$CM33") || (includeM55core &amp;&amp; vcoretype$idx eq "$CM55"))}`' />
    </Repeat>

    <!-- TFM_SP_INITIAL_ATTESTATION -->
