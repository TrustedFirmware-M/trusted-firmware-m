      <ParamString templateId='domain_custom_$idx{domainIdx}' id='read_perm' default='`${
        (pc0read ? 1 : 0) |
        ((pc1read ? 1 : 0) &lt;&lt; 1u) |
        ((pc2read ? 1 : 0) &lt;&lt; 2u) |
        ((pc3read ? 1 : 0) &lt;&lt; 3u) |
        ((pc4read ? 1 : 0) &lt;&lt; 4u) |
        ((pc5read ? 1 : 0) &lt;&lt; 5u) |
        ((pc6read ? 1 : 0) &lt;&lt; 6u) |
        ((pc7read ? 1 : 0) &lt;&lt; 7u) }`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='write_perm' default='`${
        (pc0write ? 1 : 0) |
        ((pc1write ? 1 : 0) &lt;&lt; 1u) |
        ((pc2write ? 1 : 0) &lt;&lt; 2u) |
        ((pc3write ? 1 : 0) &lt;&lt; 3u) |
        ((pc4write ? 1 : 0) &lt;&lt; 4u) |
        ((pc5write ? 1 : 0) &lt;&lt; 5u) |
        ((pc6write ? 1 : 0) &lt;&lt; 6u) |
        ((pc7write ? 1 : 0) &lt;&lt; 7u) }`' />
      <ParamString templateId='domain_custom_$idx{domainIdx}' id='ns_perm' default='`${
        (pc0ns ? 1 : 0) |
        ((pc1ns ? 1 : 0) &lt;&lt; 1u) |
        ((pc2ns ? 1 : 0) &lt;&lt; 2u) |
        ((pc3ns ? 1 : 0) &lt;&lt; 3u) |
        ((pc4ns ? 1 : 0) &lt;&lt; 4u) |
        ((pc5ns ? 1 : 0) &lt;&lt; 5u) |
        ((pc6ns ? 1 : 0) &lt;&lt; 6u) |
