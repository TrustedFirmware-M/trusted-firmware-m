    <ParamBool templateId='domain_attestation' id='pc2' name='PC 2' group='`${displayPath}`/Peripheral Protection/Accessible by:' default='true' visible='`${show_detailed_configuration}`' editable='false' desc='$PROTECTION_DOMAIN_PC_TOOLTIP' />
